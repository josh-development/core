{
  "name": "@joshdb/core",
  "version": "2.0.0",
  "description": "JavaScript Object Storage Helper",
  "author": "Ã‰velyne Lachance <eslachance@gmail.com> (https://evie.codes/)",
  "contributors": [
    "Hezekiah Hendry <hezekiah.hendry@gmail.com>",
    "DanCodes <dan@dancodes.online> (https://dancodes.online/)",
    "Wilson (https://wilson.antti.codes/)"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "clean": "rimraf coverage dist docs node_modules/.cache",
    "docs": "typedoc-json-parser",
    "lint": "eslint src tests --ext ts --fix",
    "format": "prettier --write {src,tests}/**/*.ts",
    "test": "vitest run",
    "build": "rollup -c rollup.config.ts",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run",
    "update": "yarn upgrade-interactive",
    "postinstall": "husky install .github/husky",
    "prepack": "yarn build && pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@joshdb/auto-ensure": "1.1.0-next.c54db60.0",
    "@joshdb/map": "1.1.0-next.53a85dd.0",
    "@joshdb/provider": "2.0.0-next.1a837f5.0",
    "@sapphire/utilities": "^3.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@favware/cliff-jumper": "^1.8.7",
    "@favware/npm-deprecate": "^1.0.5",
    "@favware/rollup-type-bundler": "^1.0.11",
    "@sapphire/eslint-config": "^4.3.8",
    "@sapphire/ts-config": "^3.3.4",
    "@types/node": "^17.0.23",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitest/coverage-istanbul": "^0.23.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "typedoc": "^0.23.14",
    "typedoc-json-parser": "^3.2.0",
    "typescript": "^4.8.3",
    "vitest": "^0.23.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josh-development/core.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/josh-development/core/issues"
  },
  "homepage": "https://josh.evie.dev",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": "eslint --fix --ext ts"
  },
  "packageManager": "yarn@3.2.3"
}
